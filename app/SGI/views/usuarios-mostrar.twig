<script>
$(document).ready(function(){
	var today = new Date();
	var dd = today.getDate() < 10 ? '0'+today.getDate() : today.getDate();
	var mm = (today.getMonth() + 1) < 10 ? '0'+(today.getMonth() + 1) : (today.getMonth() + 1);
	var yy = today.getFullYear();
	//$('.datepicker').datepicker({dateFormat: 'dd-mm-yy'}).val(dd+'-'+mm+'-'+yy);
	$("#finicio").datepicker({
		dateFormat: 'dd-mm-yy',
		changeYear: true,
		onSelect: function(selected) {
			$("#ftermino").datepicker("option","minDate", selected)
		}
	}).val(dd+'-'+mm+'-'+yy);
	$("#ftermino").datepicker({
		dateFormat: 'dd-mm-yy',
		changeYear: true,
		onSelect: function(selected) {
			$("#finicio").datepicker("option","maxDate", selected)
		}
	}).val(dd+'-'+mm+'-'+yy);
});

function changeModal(modal, title, id){
	console.log(modal+' '+title+' '+id);
	switch(modal){
		case '1':
			$('#pub-id-del').val(id);
			$('#pub-tit-mod').html(title);
			break;
		case '2':
			$('#proy-id-del').val(id);
			$('#proy-tit-mod').html(title);
			break;
		case '3':
			$('#tes-id-del').val(id);
			$('#tes-tit-mod').html(title);
			break;
		case '4':
			$('#cong-id-del').val(id);
			$('#cong-tit-mod').html(title);
			break;
		case '5':
			$('#exp_acad-id-del').val(id);
			//$('#exp_acad-tit-mod').html(title);
			break;
		case '6':
			$('#adm_acad-id-del').val(id);
			//$('#adm_acad-tit-mod').html(title);
			break;
		case '7':
			$('#exp_labo-id-del').val(id);
			//$('#exp_labo-tit-mod').html(title);
			break;
		case '8':
			$('#dato_extra-id-del').val(id);
			//$('#dato_extra-tit-mod').html(title);
			break;
	}
}

</script>
