{% extends "base.twig" %}
{% block title %}{{persona.name}} {{persona.last_name}} {{persona.m_name}}{% endblock %}

{% block head %}
<link href="{{ baseUrl() }}/css/token-input.css" type="text/css" rel="stylesheet">
<link href="{{ baseUrl() }}/css/jquery-ui.min.css" rel="stylesheet" type="text/css">
<link href="{{ baseUrl() }}/css/bootstrap-tokenfield.min.css" type="text/css" rel="stylesheet">
<link href="{{ baseUrl() }}/css/datepicker.css" rel="stylesheet">
<link href="{{ baseUrl() }}/css/token-input-facebook.css" type="text/css" rel="stylesheet">
{% endblock %}

{% block contentheader %}
	Perfil de Usuario 
	{% if rol == '2' %}
		<a class="pull-right btn btn-success" href="{{ siteUrl('/usuario/editar') }}/{{persona.id}}" style="color:#ffffff;"><i class="fa fa-edit"></i> Editar usuario</a>
	{% endif %}
	{% if rol == '2' or user_id == persona.id %}
		<a class="pull-right btn btn-success" href="{{ siteUrl('/usuario/cv') }}/{{persona.id}}" style="color:#ffffff;"><i class="fa fa-file"></i> Exportar CV</a>
	{% endif %}
{% endblock %}
{% block content %}
<section class="content">
	<div class="row">
		<div class="col-md-4">
			<div class="box box-primary">
				<div class="box-body box-profile">
					<img src="{{ baseUrl() }}/img/personas/{{persona.avatar}}" class="img-circle img-responsive" style="width:160px; max-width:100%;margin:auto;" alt="{{persona.name}} {{persona.last_name}} {{persona.m_name}}" title="{{persona.name}} {{persona.last_name}} {{persona.m_name}}">
					<h3 class="profile-username text-center">{{persona.name}} {{persona.last_name}} {{persona.m_name}}</h3>
					{% if profesores is empty %}
						<p class="text-muted text-center">Título No Definido</p>
					{% else %}
						{% for profesor in profesores %}
							<p class="text-muted text-center">{{ profesor.titulo_obtenido }}, {{ profesor.institucion_titulo }}, {{ profesor.pais_titulo }}</p>
							<br>
						{% endfor %}
					{% endif %}
					<ul class="list-group list-group-unbordered">
						<li class="list-group-item">
							<b>Publicaciones</b> <a class="pull-right">{{ total_publicaciones }}</a>
						</li>
						<li class="list-group-item">
							<b>Proyectos</b> <a class="pull-right">{{ total_proyectos }}</a>
						</li>
						<li class="list-group-item">
							<b>Tesis</b> <a class="pull-right">{{ total_tesis }}</a>
						</li>
					</ul>
				</div>
			</div>

			<div class="box box-primary">
				<div class="box-header with-border">
					<h3 class="box-title">Antecedentes Generales</h3>
				</div>
				<div class="box-body">
					{% if persona.type == "Estudiante" %}
						<strong><i class="fa fa-book margin-r-5"></i> Programa Actual</strong>
						<p class="text-muted">
							{{estudios.calidad}} en <a href="{{ siteUrl('/programa/perfil/')}}{{ estudios.id }} ">{{estudios.name}}</a>
						</p>
					{% endif %}
					<strong><i class="fa fa-book margin-r-5"></i>	Grado(s) Academico(s)</strong>
					{% for alumno in alumnos %}
						<p class="text-muted">{{ alumno.grado_obtenido }}</p>
					{% endfor %}
					<hr>
					<strong><i class="fa fa-map-marker margin-r-5"></i> Institución:</strong>
					<p class="text-muted">{{ persona.institucion }}</p>
					<hr>
					<strong><i class="fa fa-file-text-o margin-r-5"></i> Programa Asociado</strong>
					<p>{{ claustro | raw }}</p>
					<hr>
					<strong><i class="fa fa-pencil margin-r-5"></i> Área(s):</strong>
					<p>
						<br>
						{% if exalumnos is empty %}
							No Definida
						{% else %}
							{% for exalumno in exalumnos %}
								<span class="label label-info">{{exalumno.area}}</span>
							{% endfor %}
						{% endif %}
					</p>
					<!--p>
						<br>
						{% if areas is empty %}
							No Definida
						{% else %}
							{% for a in areas %}
								<span class="label label-info">{{ a }}</span>
							{% endfor %}
						{% endif %}
					</p-->
					<hr>
					<strong><i class="fa fa-file-text-o margin-r-5"></i> Líneas de Investigación</strong>
					<br><br>
					{% if perfil_lineas_investigativas is empty %}
						No Definido
					{% else %}
						{% for linea_investigativa in perfil_lineas_investigativas %}
							<p><span class="label label-info">{{linea_investigativa}}</span></p>
						{% endfor %}
					{% endif %}
				</div>
			</div>
		</div>

		<section class="col-lg-8 connectedSortable">
			<div class="box box-success">
				<div class="box-header">
					<h3 style="font-size:24px;" class="box-title"><i class="fa fa-comments-o"></i> Resumen de Actividad</h3>
					<div class="box-tools pull-right" data-toggle="tooltip" title="Status">
					</div>
				</div>
				<div class="box-body chat" id="chat-box">
					<div class="row">
						<div class="col-xs-12">
							<h3 id="titlepub"><i class="fa fa-book" aria-hidden="true"></i> Publicaciones</h3>
						</div>
						{% if total_publicaciones > 0 %}
							<div class="col-xs-12" id="cant-pub" data-val="{{ total_publicaciones }}">
								<strong>Publicaciones Totales: </strong>{{ total_publicaciones }}<br>
							</div>
							<div class="col-xs-6" style="padding-top:40px;">
								<label for = "myChart">
									Cantidad de publicaciones en el tiempo<br />
									<canvas data-id="{{ profesores[0].id }}" id="myChart" width="400" height="200"></canvas>
								</label>
							</div>
							<div class="col-xs-12">
								<div class="collapse" id="collapseExample">
									<div class="well">
										<table class="table">
											<thead>
												<tr>
													<th>Título</th>
													<th>Volumen</th>
													<th>Revista</th>
													<th>Año</th>
												</tr>
											</thead>
											<tbody>
												{% for publicacion in publicaciones %}
													<tr>														
														<td>{{publicacion.titulo}}</td>
														<td>{{publicacion.volumen}}</td>
														<td>{{publicacion.revista}}</td>
														<td>{{publicacion.anio}}</td>
														{% if rol == '2' or user_id == persona.id %}
															<td>
																<a href="#modal1" data-toggle="modal"><i class="fa fa-close" style="color:red" onclick="changeModal('1','{{publicacion.titulo}}','{{publicacion.id}}')"></i></a>
															</td>
														{% endif %}
													</tr>
												{% endfor %}
											</tbody>
										</table>
									</div>
									{% if rol == '2' or user_id == persona.id %}
										<div class="modal fade" id="modal1" role="dialog">
											<div class="modal-dialog">
												<div class="modal-content">
													<form action="{{ baseUrl() }}/usuarios/usuario/{{persona.id}}/eliminar-publicacion" method="POST" role="form" autocomplete="off" id="eliminar-publicacion">
														<input type="hidden" name="{{csrf_key}}" value="{{csrf_token}}">
														<div class="modal-header">
															<button type="button" class="close" data-dismiss="modal">&times;</button>
															<h4 class="modal-title">Confirmación</h4>
														</div>
														<div class="modal-body">
															<p>¿Desea borrar la publicación <b id="pub-tit-mod"></b>?</p>
														</div>
														<div class="modal-footer">
															<button id="pub-id-del" name="id" type="submit" class="btn btn-danger" onclick="$('#modal1').modal('hide')" form="eliminar-publicacion">Eliminar Publicación</button>
															<button type="button" class="btn btn-default" data-dismiss="modal">Cancelar</button>
														</div>
													</form>
												</div>
											</div>
										</div>
									{% endif %}
								</div>
							</div>
							<div class="col-xs-12">
								<button id="verpub" class="btn btn-primary pull-left"	role="button" data-toggle="collapse" href="#collapseExample" aria-expanded="false" aria-controls="collapseExample"><i class="fa fa-plus"></i> Ver Publicaciones</button>
								{% if rol == '2' or user_id == persona.id %}
									<button id="nuevapub" class="btn btn-primary pull-right" role="button" data-toggle="collapse" href="#collapsePubForm" aria-expanded="false" aria-controls="collapsePubForm"><i class="fa fa-plus"></i> Agregar Publicación</button>
								{% endif %}
							</div>
						{% else %}
							<div class="col-xs-12">
								<p>El usuario no registra publicaciones</p>
							</div>
							<div class="col-xs-12">
								{% if rol == '2' or user_id == persona.id %}
									<button id="nuevapub" class="btn btn-primary pull-right" role="button" data-toggle="collapse" href="#collapsePubForm" aria-expanded="false" aria-controls="collapsePubForm"><i class="fa fa-plus"></i> Agregar Publicación</button>
								{% endif %}
							</div>
						{% endif %}
						{% if rol == '2' or user_id == persona.id %}
							<div class="col-xs-12">
								<div class="collapse" id="collapsePubForm">
									<div class="well" style="margin-top: 20px;">
										<form action="{{ baseUrl() }}/usuarios/usuario/{{persona.id}}/agregar-publicacion" method="POST" role="form" autocomplete="off">
											<input type="hidden" name="{{csrf_key}}" value="{{csrf_token}}">
											<div class="form-group">
												<label for="titulo" class="col-sm-4 control-label">Título</label>
												<div class="col-sm-8">
													<input type="text" class="form-control"	name="titulo" required>
												</div>
											</div>
											<div class="form-group">
												<label for="volumen" class="col-sm-4 control-label">Volumen</label>
												<div class="col-sm-8">
													<input type="text" class="form-control" name="volumen" required>
												</div>
											</div>
											<div class="form-group">
												<label for="revista_id" class="col-sm-4 control-label">Revista</label>
												<div class="col-sm-8">
													<select class="form-control" name="revista_id" required>
														<option value="0" disabled selected>Revista</option>
														{% for r in revistas %}
															<option value="{{r.id}}">{{r.nombre}}</option>
														{% endfor %}
													</select>
												</div>
											</div>
											<div class="form-group">
												<label for="año" class="col-sm-4 control-label">Año</label>
												<div class="col-sm-8">
													<input type="number" class="form-control" name="año" min="1950" max="2100" required>
												</div>
											</div>
											<div class="form-group">
												<div class="col-sm-8 col-sm-offset-4">
													<input type="submit" class="form-control btn btn-primary"	name="submit" value="Agregar Publicación">
												</div>
											</div>
										</form>
									</div>
								</div>
							</div>
						{% endif %}
						<div class="col-xs-12">
							<hr>
							<h3 id="titleproy"><i class="fa fa-object-group" aria-hidden="true"></i> Proyectos</h3>
						</div>
						{% if total_proyectos > 0 %}
							<div class="col-xs-12"	id="cant-proyectos" data-val="{{ total_proyectos }}">
								<strong>Total Proyectos: </strong>{{ total_proyectos }}<br>
							</div>
							<div class="col-xs-6" style="padding-top:40px;">
								<label for = "myChart3">
									Cantidad de proyectos en el tiempo<br />
									<canvas data-id="{{ profesores[0].id }}" id="myChart3" width="400" height="200"></canvas>
								</label>
							</div>
							<div class="col-xs-12">
								<div class="collapse" id="collapseProy">
									<div id="ret-proy" class="well">
										<table class="table">
											<thead>
												<tr>
													<th>Nombre</th>
													<th>Año Inicio</th>
													<th>Año Fin</th>
													<th>Línea Investigativa</th>
													<th>Financiamiento</th>
												</tr>
											</thead>
											<tbody>
												{% for proyecto in proyectos %}
													<tr>
														<td>{{proyecto.nombre}}</td>
														<td>{{proyecto.anio_inicio}}</td>
														<td>{{proyecto.anio_fin}}</td>
														<td>{{proyecto.linea_investigativa}}</td>
														<td>{{proyecto.financiamiento}}</td>
														{% if rol == '2' or user_id == persona.id %}
															<td>
																<a href="#modal2" data-toggle="modal"><i class="fa fa-close" style="color:red" onclick="changeModal('2','{{proyecto.nombre}}','{{proyecto.id}}')"></i></a>
															</td>
														{% endif %}
													</tr>
												{% endfor %}
											</tbody>
										</table>
									</div>
									{% if rol == '2' or user_id == persona.id %}
										<div class="modal fade" id="modal2" role="dialog">
											<div class="modal-dialog">
												<div class="modal-content">
													<form action="{{ baseUrl() }}/usuarios/usuario/{{persona.id}}/eliminar-proyecto" method="POST" role="form" autocomplete="off" id="eliminar-proyecto">
														<input type="hidden" name="{{csrf_key}}" value="{{csrf_token}}">
														<div class="modal-header">
															<button type="button" class="close" data-dismiss="modal">&times;</button>
															<h4 class="modal-title">Confirmación</h4>
														</div>
														<div class="modal-body">
															<p>¿Desea borrar el proyecto <b id="proy-tit-mod"></b>?</p>
														</div>
														<div class="modal-footer">
															<button id="proy-id-del" name="id" type="submit" class="btn btn-danger" onclick="$('#modal2').modal('hide')" form="eliminar-proyecto">Eliminar Proyecto</button>
															<button type="button" class="btn btn-default" data-dismiss="modal">Cancelar</button>
														</div>
													</form>
												</div>
											</div>
										</div>
									{% endif %}
								</div>
							</div>
							<div class="col-xs-12">
								<button id="verproy" class="btn btn-primary pull-left" role="button" data-toggle="collapse" href="#collapseProy" aria-expanded="false" aria-controls="collapseProy"><i class="fa fa-plus"></i> Ver Proyectos</button>
								{% if rol == '2' or user_id == persona.id %}
									<button id="nuevoproy" class="btn btn-primary pull-right" role="button" data-toggle="collapse" href="#collapseProyForm" aria-expanded="false" aria-controls="collapseProyForm"><i class="fa fa-plus"></i> Agregar Proyecto</button>
								{% endif %}
							</div>
						{% else %}
							<div class="col-xs-12">
								<p>El usuario no registra proyectos</p>
							</div>
							<div class="col-xs-12">
								{% if rol == '2' or user_id == persona.id %}
									<button id="nuevoproy" class="btn btn-primary pull-right" role="button" data-toggle="collapse" href="#collapseProyForm" aria-expanded="false" aria-controls="collapseProyForm"><i class="fa fa-plus"></i> Agregar Proyecto</button>
								{% endif %}
							</div>
						{% endif %}
						{% if rol == '2' or user_id == persona.id %}
							<div class="col-xs-12">
								<div class="collapse" id="collapseProyForm">
									<div class="well" style="margin-top: 20px;">
										<form action="{{ baseUrl() }}/usuarios/usuario/{{persona.id}}/agregar-proyecto" method="POST" role="form" autocomplete="off">
											<input type="hidden" name="{{csrf_key}}" value="{{csrf_token}}">
											<div class="form-group">
												<label for="nombre" class="col-sm-4 control-label">Nombre Proyecto</label>
												<div class="col-sm-8">
													<input type="text" class="form-control"	name="nombre" required>
												</div>
											</div>
											<div class="form-group">
												<label for="año_inicio" class="col-sm-4 control-label">Año Inicio</label>
												<div class="col-sm-8">
													<input type="number" class="form-control" name="año_inicio" min="1950" max="2100" required>
												</div>
											</div>
											<div class="form-group">
												<label for="año_fin" class="col-sm-4 control-label">Año Fin</label>
												<div class="col-sm-8">
													<input type="number" class="form-control" name="año_fin" min="1950" max="2100" required>
												</div>
											</div>
											<div class="form-group">
												<label for="linea_id" class="col-sm-4 control-label">Línea Investigativa</label>
												<div class="col-sm-8">
													<select class="form-control" name="linea_id" required>
														<option value="0" disabled selected>Línea Investigativa</option>
														{% for l in lineas_investigativas %}
															<option value="{{l.id}}">{{l.nombre}}</option>
														{% endfor %}
													</select>
												</div>
											</div>
											<div class="form-group">
												<label for="finan_id" class="col-sm-4 control-label">Financiamiento</label>
												<div class="col-sm-8">
													<select class="form-control" name="finan_id" required>
														<option value="0" disabled selected>Financiamiento</option>
														{% for f in financiamientos %}
															<option value="{{f.id}}">{{f.nombre}}</option>
														{% endfor %}
													</select>
												</div>
											</div>
											<div class="form-group">
												<div class="col-sm-8 col-sm-offset-4">
													<input type="submit" class="form-control btn btn-primary"	name="submit" value="Agregar Proyecto">
												</div>
											</div>
										</form>
									</div>
								</div>
							</div>
						{% endif %}
						<div class="col-xs-12">
							<hr>
							<h3><i class="fa fa-archive" aria-hidden="true"></i> Tesis</h3>
						</div>
						{% if total_tesis > 0 %}
							<div class="col-xs-4">
								<strong>Total Tesis Guiadas: </strong>{{ total_tesis }}<br>
							</div>
							<!--<div class="col-xs-3">
								<strong>Pregrado: </strong>{{ cant_tipos_tesis.pregrado }}<br>
							</div>
							<div class="col-xs-3">
								<strong>Magister: </strong>{{ cant_tipos_tesis.magister }}<br>
							</div>-->
							<div class="col-xs-12">
								<div class="collapse" id="collapseTes">
									<div id="ret-tes" class="well">
										<table class="table">
											<thead>
												<tr>
													<th>Titulo</th>
													<th>Estudiante</th>
													<th>Tipo</th>
													<th>Año</th>
													<th>Participacion</th>
												</tr>
											</thead>
											<tbody>
												{% for t in tesis %}
													<tr>
														<td>{{t.titulo}}</td>
														<td>{{t.nombre_alumno}}</td>
														<td>{{t.tipo}}</td>
														<td>{{t.año}}</td>
														<td>{{t.participacion}}</td>
														{% if rol == '2' or user_id == persona.id %}
															<td>
																<a href="#modal3" data-toggle="modal"><i class="fa fa-close" style="color:red" onclick="changeModal('3','{{t.titulo}}','{{t.id}}')"></i></a>
															</td>
														{% endif %}
													</tr>
												{% endfor %}
											</tbody>
										</table>
									</div>
									{% if rol == '2' or user_id == persona.id %}
										<div class="modal fade" id="modal3" role="dialog">
											<div class="modal-dialog">
												<div class="modal-content">
													<form action="{{ baseUrl() }}/usuarios/usuario/{{persona.id}}/eliminar-tesis" method="POST" role="form" autocomplete="off" id="eliminar-tesis">
														<input type="hidden" name="{{csrf_key}}" value="{{csrf_token}}">
														<div class="modal-header">
															<button type="button" class="close" data-dismiss="modal">&times;</button>
															<h4 class="modal-title">Confirmación</h4>
														</div>
														<div class="modal-body">
															<p>¿Desea borrar la tesis <b id="tes-tit-mod"></b>?</p>
														</div>
														<div class="modal-footer">
															<button id="tes-id-del" name="id" type="submit" class="btn btn-danger" onclick="$('#modal3').modal('hide')" form="eliminar-tesis">Eliminar Tesis</button>
															<button type="button" class="btn btn-default" data-dismiss="modal">Cancelar</button>
														</div>
													</form>
												</div>
											</div>
										</div>
									{% endif %}
								</div>
							</div>
							<div class="col-xs-12">
								<button id="vertes" class="btn btn-primary" role="button" data-toggle="collapse" href="#collapseTes" aria-expanded="false" aria-controls="collapseTes"><i class="fa fa-plus"></i> Ver Tesis</button>
								{% if rol == '2' or user_id == persona.id %}
									<button id="nuevates" class="btn btn-primary pull-right" role="button" data-toggle="collapse" href="#collapseTesForm" aria-expanded="false" aria-controls="collapseTesForm"><i class="fa fa-plus"></i> Agregar Tesis</button>
								{% endif %}
							</div>
						{% else %}
							<div class="col-xs-12">
								<p>El usuario no registra tesis guiadas</p>
							</div>
							<div class="col-xs-12">
								{% if rol == '2' or user_id == persona.id %}
									<button id="nuevates" class="btn btn-primary pull-right" role="button" data-toggle="collapse" href="#collapseTesForm" aria-expanded="false" aria-controls="collapseTesForm"><i class="fa fa-plus"></i> Agregar Tesis</button>
								{% endif %}
							</div>
						{% endif %}
						{% if rol == '2' or user_id == persona.id %}
							<div class="col-xs-12">
								<div class="collapse" id="collapseTesForm">
									<div class="well" style="margin-top: 20px;">
										<form action="{{ baseUrl() }}/usuarios/usuario/{{persona.id}}/agregar-tesis" method="POST" role="form" autocomplete="off">
											<input type="hidden" name="{{csrf_key}}" value="{{csrf_token}}">
											<div class="form-group">
												<label for="titulo" class="col-sm-4 control-label">Título</label>
												<div class="col-sm-8">
													<input type="text" class="form-control"	name="titulo" required>
